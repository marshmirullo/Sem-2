    public static <E extends Comparable<E>> String maxEmployee(ShopLot{} list){
    ShopLot max = list[0];
            for (int i = 1; i < list.length; i++) {
                if(max.compareTo(list[i]>0)){
                    max = list[i];
                }
                return max;
            }
    }
    public static <E extends Comparable<E>> String mostExpensive(ShopLot[] list){
        E max=list[0];
        max=list[0];
        for (int i = 1; i < list.length; i++) {
            if(max.compareTo(list[i])<0)
                max=list[i];
        }
        return max;
    }

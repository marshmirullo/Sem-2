    public static String max1(int a,int b,int c){
        if(a>b&&a>c)
            return "McDonalds";
        else if(b>c&&b>a)
            return "Sushi Mentai";
        else
            return " Waroeng Penyet";
    }
    
    public static String max2(int a,int b,int c){
        if(a>b&&a>c)
            return "Uniqlo";
        else if(b>c&&b>a)
            return "Padini";
        else
            return "Mango";
    }
    
    public static int max(int a,int b){
        if(a>b)
            return a;
        else if(b>a)
                return b;
        else
            return 0;
        
    }
